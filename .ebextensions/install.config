container_commands:
    00_own:
        command: "chown webapp -R /var/"
    01_free:
        command: "chmod 777 -R /var/"
    03_npmglobals:
        command: "sudo npm i"
files:
    "/etc/nginx/conf.d/proxy.conf":
      mode: "000755"
      owner: root
      group: root
      content: |
        client_max_body_size 25M;